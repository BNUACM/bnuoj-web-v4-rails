<div class="row">
  <div class="col-sm-12">
    <% if logged_in? %>
      <button id="arrangevirtual" class="btn btn-primary">Arrange VContest</button>
    <% end %>
    <div class="btn-group virtual-btns">
      <button class="btn btn-default" is-virtual="">All</button>
      <button class="btn btn-default" is-virtual="0">Standard</button>
      <button class="btn btn-default" is-virtual="1">Virtual</button>
    </div>
    <div class="btn-group type-btns">
      <button class="btn btn-default" contest-type="all">All</button>
      <button class="btn btn-default" contest-type="0">ICPC</button>
      <button class="btn btn-default" contest-type="1">CF</button>
      <button class="btn btn-default" contest-type="99">Replay</button>
      <button class="btn btn-default" contest-type="-99">Non-Replay</button>
    </div>
    <div class="btn-group access-btns">
      <button class="btn btn-default" access="">All</button>
      <button class="btn btn-default" access="0">Public</button>
      <button class="btn btn-default" access="1">Private</button>
      <button class="btn btn-default" access="2">Password</button>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-sm-12">
    <table class="table table-hover table-striped" id="contestlist">
      <thead>
        <tr>
          <th> CID </th>
          <th width="30%"> Title </th>
          <th> Start Time </th>
          <th> End Time </th>
          <th> Status </th>
          <th> Access </th>
          <th> Manager </th>
          <th> Private </th>
          <th> Type </th>
        </tr>
      </thead>
      <tbody>
      </tbody>
      <tfoot>
      </tfoot>
    </table>
  </div>
</div>